{
  "name": "rpc-reflector",
  "version": "1.3.4",
  "description": "Call methods on an object over RPC",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "tap --no-check-coverage test/**/*.test.js",
    "coverage": "mkdir -p coverage && tap --no-check-coverage --coverage-report=text-lcov > ./coverage/lcov.info",
    "type": "tsc --noEmit",
    "format": "prettier --write .",
    "prepack": "tsc --outDir dist --allowJs --emitDeclarationOnly --declaration index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "Gregor MacLennan",
  "license": "ISC",
  "dependencies": {
    "@msgpack/msgpack": "^1.12.1",
    "assert": "^2.0.0",
    "duplexify": "^4.1.2",
    "is-stream": "^2.0.1",
    "length-prefixed-stream": "^2.0.0",
    "p-timeout": "^4.1.0",
    "pump": "^3.0.0",
    "serialize-error": "^8.1.0",
    "through2": "^4.0.2",
    "validate.io-array-like": "^1.0.2"
  },
  "devDependencies": {
    "@types/concat-stream": "^1.6.1",
    "@types/duplexify": "^3.6.0",
    "@types/node": "^13.9.8",
    "@types/once": "^1.4.0",
    "@types/pump": "^1.1.1",
    "@types/readable-stream": "^2.3.11",
    "@types/tape": "^4.2.34",
    "@types/tape-async": "^2.3.1",
    "@types/through2": "^2.0.36",
    "husky": "^4.2.5",
    "into-stream": "^6.0.0",
    "native-duplexpair": "^1.0.0",
    "p-is-promise": "^3.0.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "readable-error": "^1.0.0",
    "readable-stream": "^3.6.0",
    "tap": "^15.0.10",
    "tape-async": "^2.3.0",
    "type-fest": "^2.3.4",
    "typescript": "^4.4.3"
  },
  "files": [
    "server.js",
    "encode-decode.js",
    "client.js",
    "CHANGELOG.md",
    "lib/",
    "examples/",
    "dist/"
  ],
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gmaclennan/rpc-reflector.git"
  },
  "bugs": {
    "url": "https://github.com/gmaclennan/rpc-reflector/issues"
  },
  "homepage": "https://github.com/gmaclennan/rpc-reflector#readme"
}
